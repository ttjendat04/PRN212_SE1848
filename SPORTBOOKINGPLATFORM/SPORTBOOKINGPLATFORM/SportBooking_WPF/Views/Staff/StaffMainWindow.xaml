<Window x:Class="SportBooking_WPF.Views.Staff.StaffMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SportBooking_WPF.Views.Staff"
        mc:Ignorable="d"
        Title="StaffMainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10">
            <Button Content="Booking" Name="btnBooking" Margin="10" Padding="10" Click="btnBooking_Click"/>
            <Button Content="User" Name="btnUser" Margin="10" Padding="10" Click="btnUser_Click"  />
        </StackPanel>
    </Grid>
</Window>
